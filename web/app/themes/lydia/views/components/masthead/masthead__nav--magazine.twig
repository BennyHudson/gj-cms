{#
* Masthead Nav Magazines
* --------
* @package components/masthead
* @version 1.0.0
#}

<div class="c-masthead-nav__subnav c-masthead-nav-mag">
    <div class="c-masthead-nav-mag__items">
        {% for mag in nav.magazine %}
            <div class="c-masthead-nav-mag__item">
                <a class="c-masthead-nav-mag__link" href="{{ mag.link }}" tabindex="-1">
                    {% include 'objects/image.twig' with {
                        class: 'c-masthead-nav-mag__feat',
                        alt :  mag.title,
                        img : {
                            base : {
                                obj : Image(mag.thumbnail),
                                w   : '185',
                                h   : '242'
                            }
                        }
                    } %}
                </a>
            </div>
        {% endfor %}
    </div>
</div>
